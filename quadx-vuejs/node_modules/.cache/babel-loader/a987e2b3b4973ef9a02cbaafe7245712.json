{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AddCustomer',\n\n  data() {\n    return {\n      customer: {},\n      firstName: '',\n      lastName: '',\n      photoURL: '',\n      address: '',\n      mobile: '',\n      email: '',\n      dateOfBirth: '',\n      error: []\n    };\n  },\n\n  methods: {\n    async addCustomer() {\n      this.errors = [];\n\n      if (!this.errors.length) {\n        //no errors\n        let formData = new formData(); //append the data\n\n        formData.append('firstName', this.customer.firstName);\n        formData.append('lastName', this.customer.lastName);\n        formData.append('photoURL', this.customer.photoURL);\n        formData.append('address', this.customer.address);\n        formData.append('mobile', this.customer.mobile);\n        formData.append('email', this.customer.email);\n        formData.append('dateOfBirth', this.customer.dateOfBirth);\n        let url = 'http://127.0.0.1:8000/api/new_customer';\n        await axios.post(url, formData).then(response => {\n          console.log(response);\n\n          if (response.status == 200) {\n            //refresh the values\n            this.contact.firstName = '';\n            this.contact.lastName = '';\n            this.contact.photoURL = '';\n            this.contact.address = '';\n            this.contact.mobile = '';\n            this.contact.email = '';\n            this.contact.dateOfBirth = ''; //display success message\n\n            alert(response.data.message);\n          }\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEI,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,aADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,SAAS,EAAE,EAFR;MAGHC,QAAQ,EAAE,EAHP;MAIHC,QAAQ,EAAE,EAJP;MAKHC,OAAO,EAAE,EALN;MAMHC,MAAM,EAAE,EANL;MAOHC,KAAK,EAAE,EAPJ;MAQHC,WAAW,EAAE,EARV;MASHC,KAAK,EAAE;IATJ,CAAP;EAWH,CAdU;;EAeXC,OAAO,EAAE;IACL,MAAMC,WAAN,GAAoB;MAChB,KAAKC,MAAL,GAAY,EAAZ;;MACA,IAAG,CAAC,KAAKA,MAAL,CAAYC,MAAhB,EA