bmit.prevent=\"addCustomer()\" >\r\n                <!--\r\n                    @submit.prevent - prevents the form from refreshing upon submit\r\n                -->\r\n                    <fieldset>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">First Name: </label>\r\n                            <input type=\"text\" class=\"form-control\" placeholder=\"First Name\" v-model=\"customer.firstName\" required />\r\n                            <!-- v-mopdel: get value of the input element -->\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Last Name: </label>\r\n                            <input type=\"text\" class=\"form-control\" placeholder=\"Last Name\" v-model=\"customer.lastName\" required />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Photo URL: </label>\r\n                            <input type=\"text\" class=\"form-control\" placeholder=\"Photo URL\" v-model=\"customer.photoURL\" required />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Address: </label>\r\n                            <input type=\"text\" class=\"form-control\" placeholder=\"Address\" v-model=\"customer.address\" required />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Mobile Number: </label>\r\n                            <input type=\"number\" class=\"form-control\" placeholder=\"Mobile Number\" v-model=\"customer.mobile\" required />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Email Address: </label>\r\n                            <input type=\"email\" class=\"form-control\" placeholder=\"Email Address\" v-model=\"customer.email\" required />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <label class=\"form-label\">Date of Birth: </label>\r\n                            <input type=\"date\" class=\"form-control\" v-model=\"customer.dateOfBirth\" />\r\n                        </div>\r\n                        <div class=\"form-group mb-2\">\r\n                            <input type=\"submit\" class=\"btn btn-success mt-4\" value=\"Create New Customer\"/>\r\n                        </div>\r\n                    </fieldset>       \r\n                </form>\r\n            </div>\r\n            <!--image-->\r\n            <div class=\"col-md-4\">\r\n                <img :src=\"customer.photoURL\" alt=\"\" class=\"customer-img\" />\r\n                <!--dymamic display of photo-->\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    export default {\r\n        name: 'AddCustomer',\r\n        data() {\r\n            return {\r\n                customer: {},\r\n                firstName: '',\r\n                lastName: '',\r\n                photoURL: '',\r\n                address: '',\r\n                mobile: '',\r\n                email: '',\r\n                dateOfBirth: ''\r\n            }\r\n        },\r\n        methods: {\r\n            async addCustomer() {\r\n            //no errors\r\n            let formData = new FormData();\r\n            //append the data\r\n            formData.append('firstName', this.customer.firstName);\r\n            formData.append('lastName', this.customer.lastName);\r\n            formData.append('photoURL', this.customer.photoURL);\r\n            formData.append('address', this.customer.address);\r\n            formData.append('mobile', this.customer.mobile);\r\n            formData.append('email', this.customer.email);\r\n            formData.append('dateOfBirth', this.customer.dateOfBirth);\r\n            let url = 'http://127.0.0.1:8000/api/new_customer';\r\n            await axios.post(url, formData).then((response) => {\r\n                console.log(response);\r\n                if (response.status == 200) {\r\n                    //refresh the values\r\n                    this.customer.firstName = '';\r\n                    this.customer.lastName = '';\r\n                    this.customer.photoURL = '';\r\n                    this.customer.address = '';\r\n                    this.customer.mobile = '';\r\n                    this.customer.email = '';\r\n                    this.customer.dateOfBirth = '';\r\n                    //display success message\r\n                    alert(response.data.message)\r\n                } else {\r\n                    //catch errors\r\n                    console.log('error');\r\n                }\r\n            });\r\n        }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}