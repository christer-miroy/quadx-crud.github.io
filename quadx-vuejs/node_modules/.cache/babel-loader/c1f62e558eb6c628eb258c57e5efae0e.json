{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AddCustomer',\n\n  data() {\n    return {\n      customer: {},\n      firstName: '',\n      lastName: '',\n      photoURL: '',\n      address: '',\n      mobile: '',\n      email: '',\n      dateOfBirth: ''\n    };\n  },\n\n  methods: {\n    async addCustomer() {\n      //no errors\n      let formData = new FormData(); //append the data\n\n      formData.append('firstName', this.customer.firstName);\n      formData.append('lastName', this.customer.lastName);\n      formData.append('photoURL', this.customer.photoURL);\n      formData.append('address', this.customer.address);\n      formData.append('mobile', this.customer.mobile);\n      formData.append('email', this.customer.email);\n      formData.append('dateOfBirth', this.customer.dateOfBirth);\n      let url = 'http://127.0.0.1:8000/api/new_customer';\n      await axios.post(url, formData).then(response => {\n        console.log(response);\n\n        if (response.status == 200) {\n          //refresh the values\n          this.customer.firstName = '';\n          this.customer.lastName = '';\n          this.customer.photoURL = '';\n          this.customer.address = '';\n          this.customer.mobile = '';\n          this.customer.email = '';\n          this.customer.dateOfBirth = ''; //display success message\n\n          alert(response.data.message);\n        } else {\n          //catch errors\n          console.log('error');\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEI,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,aADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,SAAS,EAAE,EAFR;MAGHC,QAAQ,EAAE,EAHP;MAIHC,QAAQ,EAAE,EAJP;MAKHC,OAAO,EAAE,EALN;MAMHC,MAAM,EAAE,EANL;MAOHC,KAAK,EAAE,EAPJ;MAQHC,WAAW,EAAE;IARV,CAAP;EAUH,CAbU;;EAcXC,OAAO,EAAE;IACL,MAAMC,WAAN,GAAoB;MACpB;MACA,IAAIC,QAAO,GAAI,IAAIC,QAAJ,EAAf,CAFoB,CAGpB;;MACAD,QAAQ,CAACE,MAAT,CAAgB,WAAhB,EAA6B,KAAKZ,QAAL,CAAcC,SAA3C;MACAS,QAAQ,CAACE,MAAT,